xithrius.cloud {
    reverse_proxy xithriuscloud_site_1:8000
}

cloudflare.xithrius.cloud {
    reverse_proxy xithriuscloud_cloudflare_1:8080
}

cf.xithrius.cloud {
	redir https://cloudflare.xithrius.cloud{uri}
}

grafana.xithrius.cloud {
    reverse_proxy xithriuscloud_grafana_1:3000
}

git.xithrius.cloud {
	redir https://gitlab.xithrius.cloud{uri}
}

gitlab.xithrius.cloud {
    reverse_proxy xithriuscloud_gitlab_1:8929
}
